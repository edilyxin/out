<?xml version="1.0" encoding="UTF-8"?>   
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN" "http://www.ibatis.com/dtd/sql-map-config-2.dtd">    

<sqlMapConfig> 
    <settings 
    	cacheModelsEnabled="true" 
    	enhancementEnabled="true"  
        lazyLoadingEnabled="false" 
        maxRequests="20" 
        maxSessions="20"  
        maxTransactions="20" 
        useStatementNamespaces="false" />   
<!-- 定义类别名 -->
    <sqlMap resource="map/Sql.xml"/>
<!-- 放大镜 -->
    <sqlMap resource="map/Sql-Sys-Tree.xml"/>
<!-- 登录管理 -->
    <sqlMap resource="map/Sql-Sys-Login.xml"/>
<!-- 系统集成管理 -->
    <sqlMap resource="map/Sql-Sys-Sysint.xml"/>
<!-- 功能管理 -->
    <sqlMap resource="map/Sql-Sys-Right.xml"/>
<!-- 操作管理 -->
    <sqlMap resource="map/Sql-Sys-Oper.xml"/>
<!-- 参数管理 -->
    <sqlMap resource="map/Sql-Sys-Parm.xml"/>
<!-- 角色管理 -->
    <sqlMap resource="map/Sql-Sys-Role.xml"/>
<!-- 用户管理 -->
    <sqlMap resource="map/Sql-Sys-User.xml"/>
<!-- 字典管理 -->
    <sqlMap resource="map/Sql-Sys-Dict.xml"/>
<!-- 日志管理 -->
    <sqlMap resource="map/Sql-Sys-Log.xml"/>
<!-- 编码管理 -->
    <sqlMap resource="map/Sql-Sys-Sequence.xml"/>

<!-- 校区管理 -->
    <sqlMap resource="map/Sql-Base-School.xml"/>
<!-- 单位部门管理 -->
    <sqlMap resource="map/Sql-Base-UnitDepart.xml"/>
<!-- 岗位管理 -->
    <sqlMap resource="map/Sql-Base-Job.xml"/>
<!-- 职称管理 -->
    <sqlMap resource="map/Sql-Base-Post.xml"/>     
<!-- 职工管理 -->
    <sqlMap resource="map/Sql-Base-Emp.xml"/>
 </sqlMapConfig> 
